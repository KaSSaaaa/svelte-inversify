{
	"name": "svelte-inversify",
	"version": "0.0.5",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.22.4",
		"@sveltejs/package": "^2.2.0",
		"inversify": "^6.0.1",
		"svelte": "^4.1.2",
		"svelte-check": "^3.4.6",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.7",
		"vitest": "^0.33.0"
	},
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/KaSSaaaa/svelte-inversify.git"
	},
	"keywords": [
		"svelte",
		"inversify"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	}
}
